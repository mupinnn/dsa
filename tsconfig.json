{
  "compilerOptions": {
    "lib": ["ES2021"],
    "target": "ES2021",
    "module": "CommonJS",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "noImplicitAny": true,
    "removeComments": true
  },
  "exclude": ["node_modules"]
}
